{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#b16286",
          "style": "plain",
          "template": "\n{{ .UserName }}'s ",
          "type": "session"
        },
        {
          "foreground": "#98971a",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "\ue5ff {{ .Path }}/ ",
          "type": "path"
        },
        {
          "foreground": "#cc241d",
          "properties": {
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#ebdbb2>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#689d6a",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ebdbb2>via</> \ue781 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#d79921",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ebdbb2>-</> \ue235 {{ .Full }}",
          "type": "python"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#b16286",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#83a598{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fb4934{{end}}",
            "{{if eq \"Full\" .State.String}}#b8bb26{{end}}"
          ],
          "leading_diamond": " \ue0b6",
          "properties": {
            "charged_icon": " ",
            "charging_icon": "\u21e1 ",
            "discharging_icon": "\u21e3 "
          },
          "style": "plain",
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} ",
          "trailing_diamond": "\ue0b4",
          "type": "battery"
        },
        {
          "foreground": "#98971a",
          "leading_diamond": "\u250b",
          "style": "diamond",
          "template": " {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB ",
          "trailing_diamond": " ",
          "type": "sysinfo"
        },
        {
          "foreground": "#ebdbb2",
          "leading_diamond": "\u250b",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#d79921",
          "style": "plain",
          "template": "</b><#b16286>\u276f</><#98971a>\u276f</><#cc241d>\u276f</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
